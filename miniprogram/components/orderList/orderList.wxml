<!--components/orderList/orderList.wxml-->
<view class="item" wx:for="{{orderList}}" wx:key="index" data-index="{{index}}" bindtap="click">
  <view class="topContent">
    <view class="orderId">订单号：{{item.orderId}}</view>
    <view class="orderTime">{{item.orderTime}}</view>
  </view>
  <view class="cover">
    <image src="{{item.image}}" mode="widthFix"></image>
  </view>
  <view class="itemTitle">{{item.title}}</view>
  <view class="classify">{{item.classify}}</view>
  <view class="way">{{item.way}}</view>
</view>
